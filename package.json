{
  "name": "scrutinizer",
  "version": "1.3.2",
  "description": "Extract a git repository's metadata relying on open source conventions",
  "main": "lib/index.js",
  "homepage": "https://github.com/resin-io-modules/scrutinizer",
  "repository": {
    "type": "git",
    "url": "git@github.com:resin-io-modules/scrutinizer.git"
  },
  "scripts": {
    "test": "npm run lint && jest",
    "readme": "jsdoc2md --template doc/README.hbs lib/index.js > README.md",
    "lint": "eslint --ignore-pattern /__test__/repositories/* lib test",
    "lint-fix": "eslint --ignore-pattern /__test__/repositories/* lib test --fix"
  },
  "jest": {
    "collectCoverage": true,
    "verbose": true,
    "setupFilesAfterEnv": [
      "jest-chain"
    ],
    "testPathIgnorePatterns": ["/node_modules/", "/__tests__/repositories/"]
  },
  "author": "Resin Inc. <hello@resin.io>",
  "license": "Apache-2.0",
  "devDependencies": {
    "eslint": "^4.5.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsdoc": "^3.1.2",
    "eslint-plugin-lodash": "^2.4.5",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^24.5.0",
    "jest-chain": "^1.1.2",
    "jsdoc-to-markdown": "^3.0.4"
  },
  "dependencies": {
    "@octokit/rest": "^16.0.0",
    "bluebird": "^3.5.1",
    "debug": "^3.1.0",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "simple-git": "^1.85.0",
    "tmp": "0.0.33"
  }
}
