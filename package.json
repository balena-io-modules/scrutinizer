{
  "name": "scrutinizer",
  "version": "5.1.0",
  "description": "Extract a git repository's metadata relying on open source conventions",
  "main": "build/lib/index.js",
  "homepage": "https://github.com/resin-io-modules/scrutinizer",
  "repository": {
    "type": "git",
    "url": "git@github.com:resin-io-modules/scrutinizer.git"
  },
  "scripts": {
    "test": "npm run lint && nyc --reporter=lcov ava",
    "watch:test": "ava --watch",
    "build": "tsc",
    "readme": "jsdoc2md --template doc/README.hbs lib/index.js > README.md",
    "lint": "balena-lint -e ts -e js --typescript --fix lib test",
    "prepublishOnly": "npm run build"
  },
  "bin": {
    "scrutinizer": "build/bin/scrutinizer.js"
  },
  "author": "Resin Inc. <hello@resin.io>",
  "license": "Apache-2.0",
  "ava": {
    "concurrency": 1,
    "verbose": true,
    "timeout": "100s",
    "files": [
      "test/**/*.spec.ts",
      "!test/repositories/**/*.spec.js"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "devDependencies": {
    "@ava/typescript": "^2.0.0",
    "@balena/lint": "^6.2.0",
    "@types/bluebird": "^3.5.36",
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^9.0.13",
    "@types/github-slugger": "^1.3.0",
    "@types/js-yaml": "^4.0.2",
    "@types/lodash": "^4.14.171",
    "@types/puppeteer": "^5.4.4",
    "@types/sharp": "^0.28.5",
    "@types/tmp": "^0.2.1",
    "@types/unzipper": "^0.10.5",
    "ava": "^3.15.0",
    "jsdoc-to-markdown": "^7.1.0",
    "nyc": "^15.1.0",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@octokit/rest": "^18.12.0",
    "bluebird": "^3.7.2",
    "capitano": "^1.9.2",
    "debug": "^4.3.3",
    "fromnow": "^3.0.0",
    "fs-extra": "^10.0.1",
    "github-slugger": "^1.4.0",
    "got": "^11.8.3",
    "js-yaml": "^4.1.0",
    "linux-os-info": "^2.0.0",
    "lodash": "^4.17.4",
    "markdown": "^0.5.0",
    "mdast-util-to-string": "^2.0.0",
    "puppeteer": "^12.0.0",
    "rehype-raw": "^5.1.0",
    "rehype-remark": "^8.1.1",
    "remark-frontmatter": "^3.0.0",
    "remark-gfm": "^1.0.0",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.1.0",
    "remark-stringify": "^9.0.1",
    "sharp": "^0.29.3",
    "simple-git": "^3.0.0",
    "tesseract.js": "^2.1.1",
    "tmp": "^0.2.1",
    "toml": "^3.0.0",
    "unified": "^9.2.2",
    "unist-util-is": "^4.1.0",
    "unist-util-visit": "^2.0.3",
    "unzipper": "^0.10.11",
    "yaml": "^1.10.2"
  },
  "versionist": {
    "publishedAt": "2022-09-12T11:52:03.157Z"
  }
}
